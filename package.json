  "keywords": [
    "table",
    "filter",
    "sort",
    "pagination",
    "data-table",
    "javascript"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jazzjazzy/enhanced-simple-table.git"
  },
  "bugs": {
    "url": "https://github.com/jazzjazzy/enhanced-simple-table/issues"
  },
  "homepage": "https://github.com/jazzjazzy/enhanced-simple-table#readme",
  "devDependencies": {
    "vite": "^6.3.5"
  },
  "vite": {
    "build": {
      "lib": {
        "entry": "src/index.js",
        "name": "EnhancedSimpleTable",
        "formats": [
          "es",
          "cjs",
          "iife"
        ],
        "fileName": "enhanced-simple-table.[format].js"
      },
      "outDir": "dist",
      "rollupOptions": {
        "external": [],
        "output": {
          "globals": {}
        }
      }
    }
  }
