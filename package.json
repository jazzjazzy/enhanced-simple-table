{
  "name": "@jazzjazzy/enhanced-simple-table",
  "version": "0.1.0",
  "description": "A highly customizable table library with advanced filtering capabilities",
  "main": "dist/enhanced-simple-table.js",
  "module": "dist/enhanced-simple-table.esm.js",
  "browser": "dist/enhanced-simple-table.min.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "vite build"
